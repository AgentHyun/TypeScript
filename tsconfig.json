{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "rootDir": "./src",
    "outDir": "./dist",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "jsx": "react-jsx", // ✅ JSX 지원 설정
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true
  }
}
